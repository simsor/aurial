{
  "name": "aurial",
  "version": "1.2.0",
  "description": "Aurial",
  "repository": "simsor/aurial",
  "engines": {
    "node": ">=4.1.1"
  },
  "main": "electron.js",
  "scripts": {
    "build-dev": "webpack --config src/webpack.dev.electron.config.js",
    "dist": "webpack --display-error-details --config src/webpack.dist.config.js",
    "watch": "webpack --progress --watch --config src/webpack.dev.config.js",
    "start": "webpack-dev-server --progress --config src/webpack.dev.config.js",
    "gui-dev": "npm run build-dev && electron ./dist/electron.js"
  },
  "author": "shrimpza",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.24.1",
    "blueimp-md5": "^2.15.0",
    "copy-webpack-plugin": "^4.6.0",
    "discord-rpc": "^3.1.4",
    "exports-loader": "^0.6.4",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.6.5",
    "jquery": "^3.5.1",
    "moment": "^2.25.3",
    "preact": "^10.4.1",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.11.5"
  },
  "devDependencies": {
    "electron": "^11.0.3"
  }
}
